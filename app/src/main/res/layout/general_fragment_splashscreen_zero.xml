<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:background="@color/color_identity"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
    <ImageView
        android:id="@+id/generalFragmentSplashScreenZero_imageView_logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="150dp"
        android:layout_centerHorizontal="true"
        android:src="@mipmap/logo"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Welcome"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        android:layout_below="@id/generalFragmentSplashScreenZero_imageView_logo"
        android:id="@+id/textView"/>
    <Button
        android:id="@+id/generalFragmentSplashScreenZero_button_startSplashScreen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:padding="15dp"
        android:text="get started"
        android:layout_marginTop="24dp"
        android:layout_below="@+id/textView"
        android:layout_centerHorizontal="true"/>
    <Button
        android:id="@+id/generalFragmentSplashScreenZero_button_noThanksSplashScreen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00000000"
        android:text="no thanks"
        android:layout_below="@+id/generalFragmentSplashScreenZero_button_startSplashScreen"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="22dp"/>

</RelativeLayout>