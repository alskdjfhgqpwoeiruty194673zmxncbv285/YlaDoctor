<com.ramotion.foldingcell.FoldingCell xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:folding-cell="http://schemas.android.com/apk/res-auto"
    android:id="@+id/appointmentHistoryItem_foldingCell_container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    folding-cell:additionalFlipsCount="4"
    folding-cell:animationDuration="1500"
    folding-cell:backSideColor="#FF5252">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone">

        <include layout="@layout/patient_appointment_history_expanded" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <include layout="@layout/doctor_appointment_history_collapsed" />
    </FrameLayout>
</com.ramotion.foldingcell.FoldingCell>
